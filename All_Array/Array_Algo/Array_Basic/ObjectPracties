class WorldBank{

  constructor(name , money ){
    this.BankName=name
this.TotalMoney=money
  }


SecondWithdraw(amount){ /// change in Object 
     this.TotalMoney -= amount;
    console.log(`‚úÖ Withdrawn Rs.${amount} from ${this.BankName}`);
    console.log(`üí∞ Remaining Balance: Rs.${this.TotalMoney}`);
}


}
const BankInfo=new WorldBank('BankOfIndia' , 10000)

// BankInfo.UserWithdraw(1000)
BankInfo.SecondWithdraw(1000)
console.log(BankInfo)

// Use Client ------------------------->>

// ‚úÖ Subclass (inherits WorldBank)
class Usa extends WorldBank {
  constructor(name, money) {
    super(name, money); // must call parent constructor first
  }

  UsaWithdraw(amout){
    this.TotalMoney-=amout
    console.log(this.BankName)

  }
}

// ‚úÖ Child class object
const useuser = new Usa('USABank', 5000);
useuser.UsaWithdraw(1000)
console.log(useuser.TotalMoney);


// üîç Step-by-Step Difference
// üü© Line (1): const user = this.TotalMoney - famout;

// This calculates a new value (10000 - 1000 = 9000)

// But it does NOT change the actual class property (this.TotalMoney).

// So after this line:

// user = 9000

// this.TotalMoney = 10000 (still unchanged)

// This is just a temporary subtraction stored in a local variable.

// üü¶ Line (2): this.TotalMoney -= famout;

// This is short for:

// this.TotalMoney = this.TotalMoney - famout;


// ‚úÖ It actually updates the property in your object.

// So after this line:

// this.TotalMoney becomes 9000

// The object is mutated (changed permanently)

// ‚öôÔ∏è So in your code:
// const user = this.TotalMoney - famout;
// this.TotalMoney -= famout;
// console.log(user, 'this Total Money', this.TotalMoney);


// ‚úÖ Output will be:

// 9000 'this Total Money' 9000


// Because both user (temporary variable) and this.TotalMoney (updated property) end up having the same value ‚Äî but they were achieved differently.


// | Expression                              | Meaning                            | Changes object? | Example Result                           |
// | --------------------------------------- | ---------------------------------- | --------------- | ---------------------------------------- |
// | `const user = this.TotalMoney - famout` | Calculates subtraction only        | ‚ùå No            | `user = 9000`, `this.TotalMoney = 10000` |
// | `this.TotalMoney -= famout`             | Subtracts **and updates** property | ‚úÖ Yes           | `this.TotalMoney = 9000`                 |


